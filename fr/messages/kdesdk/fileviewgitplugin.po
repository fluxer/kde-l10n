msgctxt "@info:status"
msgid "Pulled branch %1 from %2 successfully."
msgstr "Branche %1 importée depuis %2 avec succès."

#: fileviewgitplugin.cpp:516
msgctxt "@info:status"
msgid "Pulling branch %1 from %2..."
msgstr "Importation de la branche %1 depuis %2..."

