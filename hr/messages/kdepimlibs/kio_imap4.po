#: imap4.cpp:614
#, kde-format
msgid "Message from %1 while processing '%2': %3"
msgstr "Poruka od %1 pri obradi '%2': %3"

#: imap4.cpp:616
#, kde-format
msgid "Message from %1: %2"
msgstr "Poruka od %1: %2"

#: imap4.cpp:940
#, kde-format
msgid ""
"The following folder will be created on the server: %1 What do you want to "
"store in this folder?"
msgstr ""
"Sljedeća mapa bit će stvorena na poslužitelju: %1 Što želite pohraniti u ovu "
"mapu?"

#: imap4.cpp:942
msgid "Create Folder"
msgstr "Napravi direktorij"

#: imap4.cpp:943
msgid "&Messages"
msgstr "Poruk&e"

#: imap4.cpp:943
msgid "&Subfolders"
msgstr "Pod&direktoriji"

#: imap4.cpp:1277
#, kde-format
msgid "Unsubscribe of folder %1 failed. The server returned: %2"
msgstr ""
"Uklanjanje pretplate na direktorij %1 nije uspjelo. Poslužitelj je "
"odgovorio: %2"

#: imap4.cpp:1298
#, kde-format
msgid "Subscribe of folder %1 failed. The server returned: %2"
msgstr ""
"Pretplaćivanje na direktorij %1 nije uspjelo. Poslužitelj je odgovorio:%2"

#: imap4.cpp:1367
#, kde-format
msgid "Changing the flags of message %1 failed with %2."
msgstr "Neuspjela promjena zastavica poruke %1 s %2."

#: imap4.cpp:1379
#, kde-format
msgid "Silent Changing the flags of message %1 failed with %2."
msgstr "Neuspjela tiha promjena zastavica poruke %1 s %2."

#: imap4.cpp:1411
#, kde-format
msgid "Changing the flags of message %1 failed."
msgstr "Neuspjela promjena zastavica poruke %1."

#: imap4.cpp:1456
#, kde-format
msgid ""
"Setting the Access Control List on folder %1 for user %2 failed. The server "
"returned: %3"
msgstr ""
"Postavljanje liste kontrole pristupa za mapu %1 za korisnika %2 nije "
"uspjelo. Poslužitelj je odgovorio: %3"

#: imap4.cpp:1475
#, kde-format
msgid ""
"Deleting the Access Control List on folder %1 for user %2 failed. The server "
"returned: %3"
msgstr ""
"Brisanje liste kontrole pristupa za mapu %1 za korisnika %2 nije uspjelo. "
"Poslužitelj je odgovorio: %3"

#: imap4.cpp:1492 imap4.cpp:1519
#, kde-format
msgid ""
"Retrieving the Access Control List on folder %1 failed. The server returned: "
"%2"
msgstr ""
"Dohvaćanje liste kontrole pristupa za mapu %1 nije uspjelo. Poslužitelj je "
"odgovorio: %2"

#: imap4.cpp:1553
#, kde-format
msgid "Searching of folder %1 failed. The server returned: %2"
msgstr "Pretraživanje mape %1 nije uspjelo. Poslužitelj je odgovorio: %2"

#: imap4.cpp:1588
#, kde-format
msgid "Custom command %1:%2 failed. The server returned: %3"
msgstr "Prilagođena naredba %1:%2 nije uspjela. Poslužitelj je odgovorio: %3"

#: imap4.cpp:1668
#, kde-format
msgid "Setting the annotation %1 on folder %2  failed. The server returned: %3"
msgstr ""
"Postavljanje napomene %1 za mapu %2 nije uspjelo. Poslužitelj je odgovorio: %"
"3"

#: imap4.cpp:1692
#, kde-format
msgid ""
"Retrieving the annotation %1 on folder %2 failed. The server returned: %3"
msgstr ""
"Dohvaćanje napomene %1 za mapu %2 nije uspjelo. Poslužitelj je odgovorio: %3"

#: imap4.cpp:1729
#, kde-format
msgid ""
"Retrieving the quota root information on folder %1 failed. The server "
"returned: %2"
msgstr ""
"Dohvaćanje administrativnih informacija o kvoti za mapu %1 nije uspjelo. "
"Poslužitelj je odgovorio: %2"

#: imap4.cpp:1785 imap4.cpp:1853
msgid "Unable to close mailbox."
msgstr "Nemogu zatvoriti sandučić."

#: imap4.cpp:1883
#, kde-format
msgid "Unable to get information about folder %1. The server replied: %2"
msgstr "Ne mogu dobaviti informacije o mapi %1. Poslužitelj je odgovorio: %2"

#: imap4.cpp:2040
#, kde-format
msgid ""
"The server %1 supports neither IMAP4 nor IMAP4rev1.\n"
"It identified itself with: %2"
msgstr ""
"Poslužitelj %1 ne podržava ni IMAP4 ni IMAP4rev1.\n"
"Poslužitelj se identificirao kao: %2"

#: imap4.cpp:2051
msgid ""
"The server does not support TLS.\n"
"Disable this security feature to connect unencrypted."
msgstr ""
"Poslužitelj ne podržava TLS.\n"
"Onemogućite ovu siguronosnu mogućnost za nekriptirano spajanje."

#: imap4.cpp:2075
msgid "Starting TLS failed."
msgstr "Pokretanj TLS-a nije uspjelo."

#: imap4.cpp:2085
#, kde-format
msgid "The authentication method %1 is not supported by the server."
msgstr "Poslužitelj ne podržava metodu autentifikacije %1."

#: imap4.cpp:2112
msgid "Username and password for your IMAP account:"
msgstr "Korisničko ime i zaporka vašeg IMAP-računa:"

#: imap4.cpp:2126
#, kde-format
msgid ""
"Unable to login. Probably the password is wrong.\n"
"The server %1 replied:\n"
"%2"
msgstr ""
"Neuspjela prijava. Vjerojatno zaporka nije ispravna.\n"
"Poslužitelj %1 je odgovorio:\n"
"%2"

#: imap4.cpp:2132
#, kde-format
msgid ""
"Unable to authenticate via %1.\n"
"The server %2 replied:\n"
"%3"
msgstr ""
"Autentifikacija preko %1 nije uspjela.\n"
"Poslužitelj %2 je odgovorio:\n"
"%3"

#: imap4.cpp:2622
#, kde-format
msgid "Unable to open folder %1. The server replied: %2"
msgstr "Ne mogu otvoriti mapu %1. Poslužitelj je odgovorio: %2"
