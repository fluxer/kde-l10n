#. i18n: ectx: property (text), item, widget (KComboBox, desktopSwitchCombo)
#: rc.cpp:40
msgid "Only When Moving Windows"
msgstr "Só ao Mover as Janelas"

#. i18n: file: main.ui:183
#. i18n: ectx: property (text), item, widget (KComboBox, desktopSwitchCombo)
#: rc.cpp:43
msgid "Always Enabled"
msgstr "Sempre Activo"

