#: imap4.cpp:610
#, kde-format
msgid "Message from %1 while processing '%2': %3"
msgstr "הודעה מאת %1 בעת פענוח \"%2\": %3"

#: imap4.cpp:612
#, kde-format
msgid "Message from %1: %2"
msgstr "הודעת מאת %1: %2"

#: imap4.cpp:919
#, kde-format
msgid ""
"The following folder will be created on the server: %1 What do you want to "
"store in this folder?"
msgstr "התיקייה הבאה תיווצר על השרת: %1. מה ברצונך לשמור בתיקייה זו?"

#: imap4.cpp:921
msgid "Create Folder"
msgstr "צור תיקייה"

#: imap4.cpp:922
msgid "&Messages"
msgstr "&הודעות"

#: imap4.cpp:922
msgid "&Subfolders"
msgstr "&תיקיות משנה"

#: imap4.cpp:1238
#, kde-format
msgid "Unsubscribe of folder %1 failed. The server returned: %2"
msgstr "ההסרה של תיקייה %1 נכשלה. השרת ענה: %2"

#: imap4.cpp:1257
#, kde-format
msgid "Subscribe of folder %1 failed. The server returned: %2"
msgstr "הרישום של תיקייה %1 נכשל. השרת ענה: %2."

#: imap4.cpp:1325
#, kde-format
msgid "Changing the flags of message %1 failed with %2."
msgstr "שינוי הדגלים של ההודעה %1 נכשל עם %2."

#: imap4.cpp:1335
#, kde-format
msgid "Silent Changing the flags of message %1 failed with %2."
msgstr "שינוי הדגלים של ההודעה %1 נכשל בצורה שקטה עם %2."

#: imap4.cpp:1368
#, kde-format
msgid "Changing the flags of message %1 failed."
msgstr "שינוי הדגלים של ההודעה %1 נכשל."

#: imap4.cpp:1413
#, kde-format
msgid ""
"Setting the Access Control List on folder %1 for user %2 failed. The server "
"returned: %3"
msgstr ""
"קביעת רשימת בקרת גישה על התיקייה %1 עבור משתמש %2 נכשל המשתמש. השרת החזיר: %3"

#: imap4.cpp:1429
#, kde-format
msgid ""
"Deleting the Access Control List on folder %1 for user %2 failed. The server "
"returned: %3"
msgstr "מחיקת בקרת הגישה אל ספרייה %1 עבור משתמש %2 נכשלה. השרת החזיר: %3"

#: imap4.cpp:1443 imap4.cpp:1468
#, kde-format
msgid ""
"Retrieving the Access Control List on folder %1 failed. The server returned: "
"%2"
msgstr "קבלת בקרת הגישה אל ספרייה %1 נכשלה. השרת החזיר: %2"

#: imap4.cpp:1502
#, kde-format
msgid "Searching of folder %1 failed. The server returned: %2"
msgstr "החיפוש של תיקייה %1 נכשל. השרת החזיר %2"

#: imap4.cpp:1534
#, kde-format
msgid "Custom command %1:%2 failed. The server returned: %3"
msgstr "הפקודה המיוחדת %1:%2 נכשלה. השרת החזיר: %3"

#: imap4.cpp:1612
#, fuzzy, kde-format
#| msgid ""
#| "Setting the annotation %1 on folder %2  failed. The server returned: %3"
msgid "Setting the annotation %1 on folder %2 failed. The server returned: %3"
msgstr "קביעת הביאור של %1 בתיקייה %2 נכשל. השרת החזיר: %3"

#: imap4.cpp:1633
#, kde-format
msgid ""
"Retrieving the annotation %1 on folder %2 failed. The server returned: %3"
msgstr "אחזור הביאור של %1 בתיקייה %2 נכשל. השרת החזיר %3"

#: imap4.cpp:1667
#, kde-format
msgid ""
"Retrieving the quota root information on folder %1 failed. The server "
"returned: %2"
msgstr "אחזור המידע אודות המכסה של תיקייה %1 נכשלה. השרת החזיר: %2."

#: imap4.cpp:1718 imap4.cpp:1780
msgid "Unable to close mailbox."
msgstr "אין אפשרות לסגור את תיבת הדואר."

#: imap4.cpp:1808
#, kde-format
msgid "Unable to get information about folder %1. The server replied: %2"
msgstr "אין אפשרות להשיג מידע לגבי התיקייה %1. השרת השיב: %2"

#: imap4.cpp:1963
#, kde-format
msgid ""
"The server %1 supports neither IMAP4 nor IMAP4rev1.\n"
"It identified itself with: %2"
msgstr ""
"השרת %1 לא תומך ב־IMAP4 או IMAP4rev1.\n"
"הוא הזדהה בתור: %2"

#: imap4.cpp:1975
msgid ""
"The server does not support TLS.\n"
"Disable this security feature to connect unencrypted."
msgstr ""
"השרת לא תומך ב־TLS.\n"
"בטל תכונת אבטחה זו כדי להתחבר בצורה לא מוצפנת."

#: imap4.cpp:1995
msgid "Starting TLS failed."
msgstr "הפעלת TLS נכשלה."

#: imap4.cpp:2006
#, kde-format
msgid "The authentication method %1 is not supported by the server."
msgstr "שיטת האימות %1 אינה נתמכת על ידי השרת."

#: imap4.cpp:2033
msgid "Username and password for your IMAP account:"
msgstr "שם המשתמש והססמה לחשבון ה־IMAP שלך:"

#: imap4.cpp:2046
#, kde-format
msgid ""
"Unable to login. Probably the password is wrong.\n"
"The server %1 replied:\n"
"%2"
msgstr ""
"אין אפשרות להיכנס. כנראה שהססמה שגויה.\n"
"השרת %1 השיב:\n"
"%2"

#: imap4.cpp:2052
#, kde-format
msgid ""
"Unable to authenticate via %1.\n"
"The server %2 replied:\n"
"%3"
msgstr ""
"אין אפשרות לאמת באמצעות %1.\n"
"השרת השיב %2:\n"
"%3"

#: imap4.cpp:2516
#, kde-format
msgid "Unable to open folder %1. The server replied: %2"
msgstr "אין אפשרות לפתוח את התיקייה %1. השרת השיב: %2"

#~ msgid "SASL authentication is not compiled into kio_imap4."
#~ msgstr "תמיכה באימות SASL לא קומפלה אל תוך kio_imap4."
