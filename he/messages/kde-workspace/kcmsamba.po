msgid "Date & Time"
msgstr "תאריך ושעה"

#: kcmsambalog.cpp:89 kcmsambalog.cpp:90 kcmsambastatistics.cpp:69
msgid "Event"
msgstr "אירוע"

#: kcmsambalog.cpp:89 kcmsambalog.cpp:91 kcmsambastatistics.cpp:69
