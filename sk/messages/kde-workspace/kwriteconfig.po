#: kwriteconfig.cpp:35
msgid "KWriteConfig"
msgstr "KWriteConfig"

#: kwriteconfig.cpp:37
msgid "Write KConfig entries - for use in shell scripts"
msgstr "Zápis KConfig položiek - pre použitie v shell skriptoch"

