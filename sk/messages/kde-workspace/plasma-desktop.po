#: panelcontroller.cpp:135
msgid "Panel Alignment"
msgstr "Zarovnanie panelu"

#: panelcontroller.cpp:138 panelcontroller.cpp:404
msgid "Left"
msgstr "Vľavo"

#: panelcontroller.cpp:144
msgid "Center"
msgstr "Na stred"

#: panelcontroller.cpp:149 panelcontroller.cpp:405
msgid "Right"
msgstr "Vpravo"

#: panelcontroller.cpp:160
msgid "Visibility"
msgstr "Viditeľnosť"

#: panelcontroller.cpp:163
msgid "Always visible"
msgstr "Vždy viditeľný"

#: panelcontroller.cpp:168
msgid "Auto-hide"
msgstr "Automaticky skrývať"

#: panelcontroller.cpp:173
msgid "Windows can cover"
msgstr "Môže byť prekrytý oknami"

#: panelcontroller.cpp:178
msgid "Windows go below"
msgstr "Okná idú pod"

#: panelcontroller.cpp:184
msgid "Screen Edge"
msgstr "Hrana obrazovky"

#: panelcontroller.cpp:188
msgid "Press left mouse button and drag to a screen edge to change panel edge"
msgstr ""
"Stlačte ľavé tlačidlo myši a potiahnite na okraj obrazovky na zmenu okraja "
"panelu"

#: panelcontroller.cpp:191 panelcontroller.cpp:401
msgid "Height"
msgstr "Výška"

#: panelcontroller.cpp:194
msgid "Press left mouse button and drag vertically to change panel height"
msgstr "Stlačte ľavé tlačidlo myši a potiahnite zvislo na zmenu výšky panelu"

#: panelcontroller.cpp:202
msgid "More Settings"
msgstr "Viac nastavení"

#: panelcontroller.cpp:203
msgid "Show more options about panel alignment, visibility and other settings"
msgstr "Ukázať viac možnosti o zarovnaní panelu, viditeľnosti a iné nastavenia"

