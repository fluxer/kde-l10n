msgctxt "@item:inlistbox Noise Reduction"
msgid "None"
msgstr "Žiadne"

#: libkdcraw/dcrawsettingswidget.cpp:591
msgctxt "@item:inlistbox Noise Reduction"
msgid "Wavelets"
msgstr "Vlnky"

#: libkdcraw/dcrawsettingswidget.cpp:592
msgctxt "@item:inlistbox Noise Reduction"
msgid "FBDD"
msgstr "FBDD"

#: libkdcraw/dcrawsettingswidget.cpp:593
msgctxt "@item:inlistbox Noise Reduction"
msgid "CFA Line Denoise"
msgstr "Odšumenie CFA Line"

