msgstr "Vložiť súbor"

#: insertfile/insertfileplugin.cpp:88
msgid "Insert File..."
msgstr "Vložiť súbor..."

#: insertfile/insertfileplugin.cpp:100
msgid "Choose File to Insert"
msgstr "Vyberte súbor, ktorý chcete vložiť"

#: insertfile/insertfileplugin.cpp:101
