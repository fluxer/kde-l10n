#. i18n: ectx: label, entry (ldapnosec), group (connection-$(connection))
#: kuser.kcfg:169
msgid "LDAP no encryption"
msgstr "LDAP sin cifrar"

#. i18n: ectx: label, entry (ldaptls), group (connection-$(connection))
#: kuser.kcfg:173
