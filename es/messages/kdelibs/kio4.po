#: kio/global.cpp:817
msgid "Unable to Uninitialize Input/Output Device"
msgstr "Imposible desinicializar el dispositivo de entrada/salida"

#: kio/global.cpp:818
msgid "Could Not Unmount Device"
msgstr "Imposible desmontar el dispositivo"

