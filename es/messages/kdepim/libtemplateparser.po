msgid "Set the Prefix for quoted message lines"
msgstr "Establecer el prefijo para las líneas de mensajes citados"

#. i18n: ectx: property (whatsThis), widget (KLineEdit, lineEdit_quote)
#: ui/templatesconfiguration_base.ui:293
#, no-c-format
msgctxt "@info:whatsthis"
msgid ""
"\n"
"             <qt>The following placeholders are supported in the quote "
"indicator:\n"
"             <ul>\n"
"             <li>%f: sender's initials</li>\n"
"             <li>%%: percent sign</li>\n"
"             <li>%_: space</li>\n"
"             </ul></qt>\n"
"           "
msgstr ""
"\n"
"             <qt>Los siguientes marcadores de posición se admiten en el "
"indicador de citas::\n"
"             <ul>\n"
"             <li>%f: iniciales del remitente</li>\n"
"             <li>%%: signo porcentual</li>\n"
"             <li>%_: espacio</li>\n"
"             </ul></qt>\n"
"           "
