#: weatherapplet.cpp:207
#, kde-format
msgctxt "Low temperature"
msgid "Low: %1"
msgstr "Mín: %1"

#: weatherapplet.cpp:209
#, kde-format
msgctxt "High temperature"
msgid "High: %1"
msgstr "Máx: %1"

