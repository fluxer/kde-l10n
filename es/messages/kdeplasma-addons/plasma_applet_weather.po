#, kde-format
msgctxt "ground temperature, unit"
msgid "Dewpoint: %1"
msgstr "Punto de rocío: %1"

#: weatherapplet.cpp:367
#, kde-format
msgctxt "pressure, unit"
msgid "Pressure: %1 %2"
