msgstr "Curăț %1..."

#: sweeper.cpp:145
#, kde-format
msgid "Clearing of %1 failed: %2"
msgstr "Curățarea %1 a eșuat: %2"

#: sweeper.cpp:151
msgid "Clean up finished."
msgstr "Curățirea s-a terminat."

