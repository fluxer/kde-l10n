#, kde-format
msgid ""
"This is a folder and not a file:\n"
"%1"
msgstr ""
"Isto es un dossier e non un file.\n"
"%1"

#: kfileio.cpp:77
#, kde-format
msgid ""
"You do not have read permissions to the file:\n"
"%1"
msgstr ""
"Tu non ha  permissiones de leger le file:\n"
"%1"

#: kfileio.cpp:89
#, kde-format
msgid ""
"Could not read file:\n"
"%1"
msgstr ""
"Il non pote leger le file:\n"
"%1"

#: kfileio.cpp:92
#, kde-format
msgid ""
"Could not open file:\n"
"%1"
msgstr ""
"Non pote aperir le file:\n"
"%1"

#: kfileio.cpp:95
#, kde-format
msgid ""
"Error while reading file:\n"
"%1"
msgstr ""
"Error durante que on legeva  file :\n"
"%1"

#: kfileio.cpp:113
#, kde-format
msgid "Could only read 1 byte of %2."
msgid_plural "Could only read %1 bytes of %2."
msgstr[0] "Pote solmente leger 1 byte de %2."
msgstr[1] "Pote solmente leger %1 bytes de %2."

#: kfileio.cpp:138
#, kde-format
msgid ""
"File %1 exists.\n"
"Do you want to replace it?"
msgstr ""
"File %1 existe.\n"
"Tu vole reimplaciar lo?"

#: kfileio.cpp:140 kfileio.cpp:161
msgid "Save to File"
msgstr "Salveguarda a File"

#: kfileio.cpp:141
msgid "&Replace"
msgstr "&Reimplacia"

#: kfileio.cpp:160
#, kde-format
msgid ""
"Failed to make a backup copy of %1.\n"
"Continue anyway?"
msgstr ""
"Il falleva facer un retrocopia de %1.\n"
"Continua de omne modo?"

#: kfileio.cpp:174 kfileio.cpp:190
#, kde-format
msgid ""
"Could not write to file:\n"
"%1"
msgstr ""
"Non pote scriber in file:\n"
"%1"

#: kfileio.cpp:177
#, kde-format
msgid ""
"Could not open file for writing:\n"
"%1"
msgstr ""
"Non pote aperir file per scriber:\n"
"%1"

#: kfileio.cpp:180
#, kde-format
msgid ""
"Error while writing file:\n"
"%1"
msgstr ""
"Error durante que on scribeva file :\n"
"%1"

#: kfileio.cpp:194
#, kde-format
msgid "Could only write 1 byte of %2."
msgid_plural "Could only write %1 bytes of %2."
msgstr[0] "Solmente pote scriber 1 byte de %2."
msgstr[1] "Solmente pote scriber %1 bytes de %2."

#: kfileio.cpp:221
#, kde-format
msgid "%1 does not exist"
msgstr "%1 non existe"

#: kfileio.cpp:232
#, kde-format
msgid "%1 is not accessible and that is unchangeable."
msgstr "%1 non es accessibile e ergo non es modificabile."

#: kfileio.cpp:253
#, kde-format
msgid "%1 is not readable and that is unchangeable."
msgstr "%1 non es legibile e ergo non es modificabile."

#: kfileio.cpp:269
#, kde-format
msgid "%1 is not writable and that is unchangeable."
msgstr "%1 non es scribibile e ergo non es modificabile."

#: kfileio.cpp:283
#, kde-format
msgid "Folder %1 is inaccessible."
msgstr "Dossier %1 es inaccessibile."

#: kfileio.cpp:313
msgid ""
"Some files or folders do not have the necessary permissions, please correct "
"them manually."
msgstr ""
"Alcun files o dossieres non ha le necessari permissiones, pro favor tu "
"corrige lor manualmente."

#: kfileio.cpp:316
msgid "Permissions Check"
msgstr "Verifica de permissiones"
