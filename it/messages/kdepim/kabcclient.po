#: src/main.cpp:185 src/main.cpp:351
#, kde-format
msgid "Invalid output format \"%1\". See --output-format help"
msgstr "Formato di output «%1» non valido. Vedi --output-format help"

#: src/main.cpp:195
#, kde-format
msgid ""
"Invalid options for output format \"%1\". See --output-format-options help"
msgstr ""
"Opzioni per il formato di output «%1» non valide. Vedi --output-format-"
"options help"

