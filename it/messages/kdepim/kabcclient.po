msgstr "+[dati in input]"

#: src/main.cpp:119
msgid "Input to use instead of reading stdin"
msgstr "Input da usare al posto di stdin"

#: src/main.cpp:159
msgid "No operation specified, assuming --search"
msgstr "Nessuna operazione specificata, assumo sia --search"

