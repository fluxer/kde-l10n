#: core/smartattribute.cpp:155
msgctxt "SMART attr description"
msgid "Shock During Write"
msgstr "Stød under skrivning"

#: core/smartattribute.cpp:156
msgctxt "SMART attr name"
msgid "Disk Shift"
msgstr "Diskforskydning"

#: core/smartattribute.cpp:156
msgctxt "SMART attr description"
