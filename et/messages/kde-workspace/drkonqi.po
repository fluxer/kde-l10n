msgctxt "@action:button"
msgid "Report &Bug"
msgstr "Saada &veateade"

#: drkonqidialog.cpp:159
msgctxt "@info:tooltip"
msgid "Starts the bug report assistant."
msgstr "Käivitab veast teatamise nõuandja."

#: drkonqidialog.cpp:171
