#, kde-format
msgctxt "Unsure if it is possible to attend."
msgid "Tentative: %1"
msgstr "Шартты түрде: %1"

#: text_calendar.cpp:574
#, kde-format
msgctxt "Accepted the invitation."
msgid "Accepted: %1"
msgstr "Қабыл етілген: %1"

#: text_calendar.cpp:723
msgid "Incidence with no summary"
msgstr "Тұжырмысыз"

#: text_calendar.cpp:728
#, kde-format
msgid "Answer: %1"
msgstr "Жауап: %1"

#: text_calendar.cpp:731
#, kde-format
msgid "Delegated: %1"
msgstr "Өкіл етілген: %1"

#: text_calendar.cpp:734
#, kde-format
msgid "Forwarded: %1"
msgstr "Басқаға жіберілген: %1"

