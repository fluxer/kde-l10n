msgid "query current value of start menu entry custom name string"
msgstr "Zatraži trenutnu vrednost posebne niske imena stavke start-menija"

#: main.cpp:67
msgid "set custom version string for root start menu entry"
msgstr "Postavi posebnu nisku izdanja za korenu stavku start-menija"

#: main.cpp:68
msgid "remove custom version string from root start menu entry"
msgstr "Ukloni posebnu nisku izdanja za korenu stavku start-menija"

