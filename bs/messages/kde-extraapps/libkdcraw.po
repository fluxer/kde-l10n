msgstr "Sirovo (bez profila)"

#: libkdcraw/dcrawsettingswidget.cpp:700
msgctxt "@item:inlistbox Workspace"
msgid "sRGB"
msgstr "sRGB"

#: libkdcraw/dcrawsettingswidget.cpp:701
msgctxt "@item:inlistbox Workspace"
msgid "Adobe RGB"
msgstr "Adobe RGB"

