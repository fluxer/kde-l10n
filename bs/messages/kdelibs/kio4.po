#: kio/global.cpp:839
msgid "You may not have permissions to read from the resource."
msgstr "Možda nemate dozvole za čitanje iz ovog resursa."

#: kio/global.cpp:848
msgid "Cannot Write to Resource"
msgstr "Ne mogu da pišem u resurs"

