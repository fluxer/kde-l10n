msgstr[1] "Det gick bara att läsa %1 bytes av %2."

#: kfileio.cpp:138
#, kde-format
msgid ""
"File %1 exists.\n"
"Do you want to replace it?"
msgstr ""
"Filen %1 finns.\n"
"Vill du ersätta den?"

#: kfileio.cpp:140 kfileio.cpp:161
msgid "Save to File"
msgstr "Spara till fil"

#: kfileio.cpp:141
msgid "&Replace"
msgstr "&Ersätt"

#: kfileio.cpp:160
#, kde-format
msgid ""
"Failed to make a backup copy of %1.\n"
"Continue anyway?"
msgstr ""
"Kunde inte skapa en säkerhetskopia av %1.\n"
"Fortsätta ändå?"

#: kfileio.cpp:174 kfileio.cpp:190
#, kde-format
msgid ""
"Could not write to file:\n"
"%1"
msgstr ""
"Kunde inte skriva till filen:\n"
"%1"

#: kfileio.cpp:177
#, kde-format
msgid ""
"Could not open file for writing:\n"
"%1"
msgstr ""
"Kan inte öppna filen för skrivning:\n"
"%1"

#: kfileio.cpp:180
#, kde-format
msgid ""
"Error while writing file:\n"
"%1"
msgstr ""
"Fel vid skrivning av filen:\n"
"%1"

#: kfileio.cpp:194
#, kde-format
msgid "Could only write 1 byte of %2."
msgid_plural "Could only write %1 bytes of %2."
msgstr[0] "Kunde bara skriva 1 byte av %2."
msgstr[1] "Kunde bara skriva %1 bytes av %2."

#: kfileio.cpp:221
#, kde-format
msgid "%1 does not exist"
msgstr "%1 finns inte"

#: kfileio.cpp:232
#, kde-format
msgid "%1 is not accessible and that is unchangeable."
msgstr "%1 kan inte kommas åt, och det går inte att ändra."

#: kfileio.cpp:253
#, kde-format
msgid "%1 is not readable and that is unchangeable."
msgstr "%1 kan inte läsas, och det går inte att ändra."

#: kfileio.cpp:269
#, kde-format
msgid "%1 is not writable and that is unchangeable."
msgstr "%1 kan inte skrivas, och det går inte att ändra."

#: kfileio.cpp:283
#, kde-format
msgid "Folder %1 is inaccessible."
msgstr "Katalogen %1 kan inte kommas åt."

#: kfileio.cpp:313
msgid ""
"Some files or folders do not have the necessary permissions, please correct "
"them manually."
msgstr ""
"Vissa filer eller kataloger har inte nödvändiga rättigheter. Korrigera dem "
"manuellt."

#: kfileio.cpp:316
msgid "Permissions Check"
msgstr "Behörighetskontroll"
