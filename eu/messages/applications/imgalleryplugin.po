msgstr "<i>Irudien kopurua</i>: %1"

#: imgalleryplugin.cpp:179
msgid "<i>Created on</i>: %1"
msgstr "<i>Sorrera</i>: %1"

#: imgalleryplugin.cpp:184
msgid "<i>Subfolders</i>:"
msgstr "<i>Azpikarpetak</i>:"

