#: lib/parser.cpp:538
#, kde-format
msgid "Unexpected symbol after variable '%1'."
msgstr "Nieoczekiwany symbol po zmiennej '%1'."

#: lib/parser.cpp:789
#, kde-format
msgid ""
"Expected '%1'<br><br>Possible cause of the error is having a variable with "
"the same name as a widget."
msgstr ""
"Oczekiwano '%1'<br><br>Prawdopodobną przyczyna tego błędu jest posiadanie "
"zmiennej o tej samej nazwie co element interfejsu."

#: lib/parser.cpp:791
#, kde-format
msgid "Expected '%1' got '%2'."
msgstr "Oczekiwano '%1' uzyskano '%2'."

#: lib/parser.cpp:805 lib/parser.cpp:818
