#: src/main.cpp:111
msgid "How to convert the input text."
msgstr "Jak konwertować tekst wejściowy."

#: src/main.cpp:115
msgid "How to convert the output text."
msgstr "Jak konwertować tekst wyjściowy."

#: src/main.cpp:117
msgid "Match key fields case sensitive. UID is always matched case sensitive"
msgstr ""
"Rozróżniaj wielkość liter w polach kluczy. W UID zawsze rozróżniana jest "
"wielkość liter"

#: src/main.cpp:119
msgid "+[input data]"
msgstr "+[dane wejściowe]"

#: src/main.cpp:119
msgid "Input to use instead of reading stdin"
msgstr "Wejście używane zamiast odczytu stdin"

#: src/main.cpp:159
msgid "No operation specified, assuming --search"
msgstr "Nie podano operacji, przyjęto --search"

