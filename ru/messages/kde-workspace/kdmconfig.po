msgid "Installing KDM themes"
msgstr "Установка тем KDM"

#: kdm-theme.cpp:303
msgctxt "@info:progress"
msgid "<qt>Unpacking <strong>%1</strong> theme</qt>"
msgstr "<qt>Распаковка темы <strong>%1</strong></qt>"

#: kdm-theme.cpp:315
msgctxt "@info:progress"
