msgstr "Μη έγκυρη κωδικοποίηση εισόδου \"%1\""

#: src/main.cpp:219
#, kde-format
msgid "Invalid output codec \"%1\""
msgstr "Μη έγκυρη κωδικοποίηση εξόδου \"%1\""

