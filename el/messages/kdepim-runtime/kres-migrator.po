#: kresmigratorbase.cpp:64
#, kde-format
msgid "Unable to create compatibility bridge: %1"
msgstr "Αδυναμία δημιουργίας γέφυρας συμβατότητας: %1"

#: kresmigratorbase.cpp:100
#, kde-format
msgid "Migration of '%1' to compatibility bridge failed: %2"
msgstr "Η μεταφορά του '%1' στη γέφυρα συμβατότητας απέτυχε: %2"

#: kresmigratorbase.cpp:103
#, kde-format
msgid "Migration of '%1' to native backend failed: %2"
msgstr "Η μεταφορά του '%1' στο εγγενές σύστημα υποστήριξης απέτυχε: %2"

#: kresmigratorbase.cpp:142
#, kde-format
msgid "Migration of '%1' succeeded."
msgstr "Επιτυχία μεταφοράς του '%1'."

