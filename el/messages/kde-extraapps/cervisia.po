msgid "Find Next"
msgstr "Εύρεση επόμενου"

#: annotatedialog.cpp:40
msgid "Find Prev"
msgstr "Αναζήτηση προηγούμενου"

#: annotatedialog.cpp:41
msgid "Go to Line..."
msgstr "Μετάβαση στη γραμμή..."

#: annotatedialog.cpp:51
