#: kio/global.cpp:696
msgid "Could Not Listen"
msgstr "No s'ha pogut detectar"

#: kio/global.cpp:706
msgid "Could Not Accept Network Connection"
msgstr "No es pot fer l'accept d'una connexió de xarxa"

#: kio/global.cpp:707
