msgstr "Trever Fischer"

#: DeviceModel.cpp:51
msgid "Device"
msgstr "裝置"

#: DeviceModel.cpp:53
msgid "Automount on Login"
msgstr "登入時自動掛載"

#: DeviceModel.cpp:55
msgid "Automount on Attach"
msgstr "連上時自動掛載"

#: DeviceModel.cpp:214 DeviceModel.cpp:259
msgid "UDI: %1"
msgstr "UDI: %1"

#: DeviceModel.cpp:227 DeviceModel.cpp:272
