msgid "JEDEC Units (KB, MB, etc)"
msgstr "Unitats JEDEC (KB, MB, etc.)"

#: kcmlocale.cpp:3310
msgctxt "Unit of binary measurement"
msgid "Metric Units (kB, MB, etc)"
msgstr "Unitats mètriques (kB, MB, etc.)"

#: kcmlocale.cpp:3335
