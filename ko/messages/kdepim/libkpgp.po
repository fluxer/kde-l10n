#: kpgpui.cpp:140
msgid "Autodetect"
msgstr "자동 감지"

#: kpgpui.cpp:141
msgid "GnuPG - Gnu Privacy Guard"
msgstr "GnuPG - Gnu 개인정보 보호"

#: kpgpui.cpp:142
msgid "PGP Version 2.x"
msgstr "PGP 버전 2.x"

#: kpgpui.cpp:143
msgid "PGP Version 5.x"
msgstr "PGP 버전 5.x"

#: kpgpui.cpp:144
msgid "PGP Version 6.x"
