msgstr "كىرىشنى سىنا"

#: providerconfigwidget.cpp:121
msgid "Testing login..."
msgstr "كىرىشنى سىناۋاتىدۇ…"

#: providerconfigwidget.cpp:133
msgid "Success"
msgstr "مۇۋەپپەقىيەتلىك بولدى"

#: providerconfigwidget.cpp:137
msgid "Login failed"
msgstr "كىرىش مەغلۇپ بولدى"

