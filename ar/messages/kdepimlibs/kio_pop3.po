#: pop3.cpp:215
#, kde-format
msgid "The server said: \"%1\""
msgstr "قال الخادم: \"%1\""

#: pop3.cpp:237
msgid "The server terminated the connection."
msgstr "قام الخادم بقطع الاتصال."

#: pop3.cpp:239
#, kde-format
msgid ""
"Invalid response from server:\n"
"\"%1\""
msgstr ""
"رد خاطئ من الخادم:\n"
"\"%1\""

