msgstr "&Schéma"

#: view/kateview.cpp:520
msgid "&Indentation"
msgstr "Od&sazování"

#: view/kateview.cpp:524
msgid "Select the entire text of the current document."
msgstr "Vybrat celý text současného dokumentu."

