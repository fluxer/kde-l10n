#: gstreamer/backend.cpp:215
msgid ""
"Warning: You do not seem to have the package gstreamer0.10-plugins-good "
"installed.\n"
"          Some video features have been disabled."
msgstr ""
"Wohrschoen. As't lett hest Du dat Paket \"gstreamer0.10-plugins-good\" nich "
"installeert.\n"
"          En Reeg Video-Funkschonen wöörn utmaakt."

#: gstreamer/backend.cpp:223
msgid ""
"Warning: You do not seem to have the base GStreamer plugins installed.\n"
"          All audio and video support has been disabled"
msgstr ""
"Wohrschoen. As't lett hest Du \"base\"-GStreamer-Modulen nich installeert.\n"
"          De hele Ünnerstütten för Klang un Video wöör utmaakt."

#: gstreamer/mediaobject.cpp:403
msgid "Default"
msgstr "Standard"

#: gstreamer/mediaobject.cpp:417 gstreamer/mediaobject.cpp:446
msgid "Unknown"
msgstr "Nich begäng"

#: gstreamer/mediaobject.cpp:433
msgid "Disable"
msgstr "Utmaken"

#: gstreamer/pipeline.cpp:494
msgid "One or more plugins are missing in your GStreamer installation."
msgstr "Dien GStreamer-Installatschoon bargt nich all bruukt Modulen."

#: gstreamer/plugininstaller.cpp:187
msgid "Missing codec helper script assistant."
msgstr "Kodek-Skripthölper fehlt."

#: gstreamer/plugininstaller.cpp:189
msgid "Plugin codec installation failed."
msgstr "Installeren vun Kodekmoduul fehlslaan"

#: gstreamer/plugininstaller.cpp:212
msgid "Phonon attempted to install an invalid codec name."
msgstr "Phonon hett versöcht, en leeg Kodeknaam to installeren."

#: gstreamer/plugininstaller.cpp:215
msgid "The codec installer crashed."
msgstr "Dat Kodek-Installeerprogramm is afstört."

#: gstreamer/plugininstaller.cpp:218
msgid "The required codec could not be found for installation."
msgstr "De anfraagt Kodek lett sik nich för't Installeren finnen."

#: gstreamer/plugininstaller.cpp:221
msgid "An unspecified error occurred during codec installation."
msgstr "Bi't Installeren vun den Kodek geev dat en nich begäng Fehler."

#: gstreamer/plugininstaller.cpp:224
msgid "Not all codecs could be installed."
msgstr "Nich all Kodeks laat sik installeren."

#: gstreamer/plugininstaller.cpp:227
msgid "User aborted codec installation"
msgstr "Kodek-Installatschoon vun Bruker afbraken"

#: gstreamer/plugininstaller.cpp:238
msgid "Could not update plugin registry after update."
msgstr "Moduul-Registry lett sik na't Opfrischen nich op Stand bringen."
