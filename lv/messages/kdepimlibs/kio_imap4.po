msgstr "&Vēstules"

#: imap4.cpp:922
msgid "&Subfolders"
msgstr "Apakšmape&s"

#: imap4.cpp:1238
#, kde-format
msgid "Unsubscribe of folder %1 failed. The server returned: %2"
msgstr "Neizdevās atrakstīties uz mapi %1. Serveris teica: %2"

#: imap4.cpp:1257
#, kde-format
msgid "Subscribe of folder %1 failed. The server returned: %2"
msgstr "Neizdevās pierakstīties uz mapi %1. Serveris teica: %2"

#: imap4.cpp:1325
#, kde-format
msgid "Changing the flags of message %1 failed with %2."
msgstr "Neizdevās mainīt ziņojuma %1 karogus, kļūda: %2."

