#: imap4.cpp:1368
#, kde-format
msgid "Changing the flags of message %1 failed."
msgstr "Neizdevās mainīt ziņojuma %1 karogus."

#: imap4.cpp:1413
#, kde-format
msgid ""
"Setting the Access Control List on folder %1 for user %2 failed. The server "
"returned: %3"
msgstr ""
"Neizdevās saglabāt Piekļuves Kontroles Sarakstu mapei %1, lietotājam %2. "
"Serveris teica: %3"

#: imap4.cpp:1429
#, kde-format
msgid ""
"Deleting the Access Control List on folder %1 for user %2 failed. The server "
"returned: %3"
msgstr ""
"Neizdevās izmest Piekļuves Kontroles Sarakstu mapei %1, lietotājam %2. "
"Serveris teica: %3"

#: imap4.cpp:1443 imap4.cpp:1468
#, kde-format
msgid ""
"Retrieving the Access Control List on folder %1 failed. The server returned: "
"%2"
