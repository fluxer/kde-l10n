msgstr ""

#: pop3.cpp:980
#, kde-format
msgid ""
"Error during communication with the POP3 server while trying to download "
"mail: %1"
msgstr ""

#: pop3.cpp:1042
msgid "The POP3 command 'STAT' failed"
msgstr "POP3 komanda 'STAT' neveiksmīga"

#: pop3.cpp:1048
msgid "Invalid POP3 response, should have at least one space."
msgstr "Nederīga POP3 atbilde, vajadzētu saturēt vismaz vienu atstarpi."

#: pop3.cpp:1056
msgid "Invalid POP3 STAT response."
msgstr "Nederīga POP3 STAT atbilde"

