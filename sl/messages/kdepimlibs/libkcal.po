msgctxt "@option"
msgid "Accept Attendance"
msgstr "Sprejmi udeležbo"

#: scheduler.cpp:674
msgctxt "@option"
msgid "Reject Attendance"
msgstr "Zavrni udeležbo"

#: scheduler.cpp:679
msgctxt "@info"
msgid "The organizer rejected your attendance at this meeting."
msgstr "Organizator je zavrnil vašo udeležbo na tem srečanju."

