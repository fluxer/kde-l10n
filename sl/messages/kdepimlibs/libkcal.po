msgid "%1 wants to attend %2 but was not invited."
msgstr "%1 se želi udeležiti %2, vendar ni bil povabljen."

#: scheduler.cpp:667
#, kde-format
msgctxt "@info"
msgid "%1 wants to attend %2 on behalf of %3."
msgstr "%1 se želi v imenu %3 udeležiti %2."

