#: imap4.cpp:610
#, kde-format
msgid "Message from %1 while processing '%2': %3"
msgstr "Sporočilo od %1 med obdelovanjem »%2«: %3"

#: imap4.cpp:612
#, kde-format
msgid "Message from %1: %2"
msgstr "Sporočilo od %1: %2"

#: imap4.cpp:919
#, kde-format
msgid ""
"The following folder will be created on the server: %1 What do you want to "
"store in this folder?"
msgstr ""
"Na strežniku bo ustvarjena naslednja mapa: %1 Kaj želite shraniti v njej?"

#: imap4.cpp:921
msgid "Create Folder"
msgstr "Ustvari mapo"

#: imap4.cpp:922
msgid "&Messages"
msgstr "&Sporočila"

#: imap4.cpp:922
msgid "&Subfolders"
msgstr "&Podmape"

#: imap4.cpp:1238
#, kde-format
msgid "Unsubscribe of folder %1 failed. The server returned: %2"
msgstr "Preklic naročnine na mapo %1 ni uspel. Strežnik je vrnil: %2"

#: imap4.cpp:1257
#, kde-format
msgid "Subscribe of folder %1 failed. The server returned: %2"
msgstr "Naročanje na mapo %1 ni uspelo. Strežnik je vrnil: %2"

#: imap4.cpp:1325
#, kde-format
msgid "Changing the flags of message %1 failed with %2."
msgstr "Spreminjanje zastavic sporočila %1 je spodletelo z napako %2."

#: imap4.cpp:1335
#, kde-format
msgid "Silent Changing the flags of message %1 failed with %2."
msgstr "Tiho spreminjanje zastavic sporočila %1 je spodletelo z napako %2"

#: imap4.cpp:1368
#, kde-format
msgid "Changing the flags of message %1 failed."
msgstr "Spreminjanje zastavic sporočila %1 ni uspelo."

#: imap4.cpp:1413
#, kde-format
msgid ""
"Setting the Access Control List on folder %1 for user %2 failed. The server "
"returned: %3"
msgstr ""
"Nastavljanje seznama nadzora dostopa na mapi %1 za uporabnika %2 ni uspelo. "
"Strežnik je vrnil: %3"

#: imap4.cpp:1429
#, kde-format
msgid ""
"Deleting the Access Control List on folder %1 for user %2 failed. The server "
"returned: %3"
msgstr ""
"Brisanje seznama nadzora dostopa na mapi %1 za uporabnika %2 ni uspelo. "
"Strežnik je vrnil: %3"

#: imap4.cpp:1443 imap4.cpp:1468
#, kde-format
msgid ""
"Retrieving the Access Control List on folder %1 failed. The server returned: "
"%2"
msgstr ""
"Pridobivanje seznama nadzora dostopa na mapi %1 ni uspelo. Strežnik je "
"vrnil: %2"

#: imap4.cpp:1502
#, kde-format
msgid "Searching of folder %1 failed. The server returned: %2"
msgstr "Iskanje po mapi %1 ni uspelo. Strežnik je vrnil: %2"

#: imap4.cpp:1534
#, kde-format
msgid "Custom command %1:%2 failed. The server returned: %3"
msgstr "Ukaz po meri %1:%2 ni uspel. Strežnik je vrnil: %3"

#: imap4.cpp:1612
#, kde-format
msgid "Setting the annotation %1 on folder %2 failed. The server returned: %3"
msgstr "Nastavljanje zabeležke %1 na mapi %2 ni uspelo. Strežnik je vrnil: %3"

#: imap4.cpp:1633
#, kde-format
msgid ""
"Retrieving the annotation %1 on folder %2 failed. The server returned: %3"
msgstr "Pridobivanje zabeležke %1 na mapi %2 ni uspelo. Strežnik je vrnil: %3"

#: imap4.cpp:1667
#, kde-format
msgid ""
"Retrieving the quota root information on folder %1 failed. The server "
"returned: %2"
msgstr ""
"Pridobivanje korenskih podatkov o količinskih omejitvah na mapi %1 ni "
"uspelo. Strežnik je vrnil: %2"

#: imap4.cpp:1718 imap4.cpp:1780
msgid "Unable to close mailbox."
msgstr "Poštnega predala ni bilo mogoče zapreti."

#: imap4.cpp:1808
#, kde-format
msgid "Unable to get information about folder %1. The server replied: %2"
msgstr "Ni mogoče pridobiti podatkov o mapi %1. Strežnik je odgovoril: %2"

#: imap4.cpp:1963
#, kde-format
msgid ""
"The server %1 supports neither IMAP4 nor IMAP4rev1.\n"
"It identified itself with: %2"
msgstr ""
"Strežnik %1 ne podpira ne IMAP4 ne IMAP4rev1.\n"
"Predstavlja se kot: %2"

#: imap4.cpp:1975
msgid ""
"The server does not support TLS.\n"
"Disable this security feature to connect unencrypted."
msgstr ""
"Strežnik ne podpira TLS\n"
"Onemogočite to varnostno zmožnost, če se želite povezati brez šifriranja."

#: imap4.cpp:1995
msgid "Starting TLS failed."
msgstr "Začenjanje TLS ni uspelo."

#: imap4.cpp:2006
#, kde-format
msgid "The authentication method %1 is not supported by the server."
msgstr "Način overitve %1 ni podprt na strežniku."

#: imap4.cpp:2033
msgid "Username and password for your IMAP account:"
msgstr "Uporabniško ime in geslo za vaš račun IMAP:"

#: imap4.cpp:2046
#, kde-format
msgid ""
"Unable to login. Probably the password is wrong.\n"
"The server %1 replied:\n"
"%2"
msgstr ""
"Prijava ni uspela. Najverjetneje je geslo napačno.\n"
"Strežnik %1 je odgovoril:\n"
"%2"

#: imap4.cpp:2052
#, kde-format
msgid ""
"Unable to authenticate via %1.\n"
"The server %2 replied:\n"
"%3"
msgstr ""
"Ni se mogoče overiti preko %1.\n"
"Strežnik %2 je odgovoril:\n"
"%3"

#: imap4.cpp:2516
#, kde-format
msgid "Unable to open folder %1. The server replied: %2"
msgstr "Ni mogoče odpreti mape %1. Strežnik je odgovoril: %2"
