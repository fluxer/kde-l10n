msgstr "potrebuje pravice skrbnika. "

#: kdesudo.cpp:256
msgid "Please enter your password."
msgstr "Prosimo, vnesite svoje geslo."

#: kdesudo.cpp:258
msgid "Please enter password for <b>%1</b>."
