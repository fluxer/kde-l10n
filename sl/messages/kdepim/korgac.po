msgid "Could not start KOrganizer so editing is not possible."
msgstr "KOrganizer-ja ni bilo mogoče zagnati, tako da urejanje ni mogoče."

#: alarmdialog.cpp:952
#, kde-format
msgctxt "@info"
msgid "An internal KOrganizer error occurred attempting to modify \"%1\""
msgstr ""
"Med poskusom spremembe \"%1\" je prišlo do do notranje napake v KOrganizer-"
"ju."

