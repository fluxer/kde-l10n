#: killrunner.cpp:61 killrunner_config.cpp:60 killrunner_config.cpp:86
msgid "kill"
msgstr "ពិឃាត"

#: killrunner.cpp:67
msgid "Terminate running applications whose names match the query."
msgstr "បញ្ចប់​កម្មវិធីកំពុងរត់ ដែល​ឈ្មោះ​របស់​វា​ដូច​គ្នា​នឹង​សំណួរ ។"

#: killrunner.cpp:140
#, kde-format
msgid "Terminate %1"
msgstr "បញ្ចប់ %1"

#: killrunner.cpp:141
#, kde-format
msgid ""
"Process ID: %1\n"
"Running as user: %2"
msgstr ""
"លេខ​សម្គាល់​ដំណើរការ ៖ %1\n"
"កំពុង​រត់​ជា​អ្នកប្រើ ៖ %2"

#: killrunner.cpp:206
msgid "Send SIGTERM"
msgstr "ផ្ញើ SIGTERM"

#: killrunner.cpp:207
msgid "Send SIGKILL"
msgstr "ផ្ញើ SIGKILL"

#: killrunner_config.cpp:40
msgid "CPU usage"
msgstr "ការ​ប្រើប្រាស់​ស៊ីភីយូ"

#: killrunner_config.cpp:41
msgid "inverted CPU usage"
msgstr "ការ​ប្រើប្រាស់​ស៊ីភីយូ​ដែលបាន​ដាក់បញ្ច្រាស"

#: killrunner_config.cpp:42
msgid "nothing"
msgstr "គ្មាន​អ្វីទាំងអស់"

