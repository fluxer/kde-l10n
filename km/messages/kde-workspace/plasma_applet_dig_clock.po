#: clock.cpp:263
msgid "Appearance"
msgstr "រូបរាង​"

#: clock.cpp:278
msgctxt "A kind of date representation"
msgid "No date"
msgstr "គ្មាន​កាលបរិច្ឆេទ​"

#: clock.cpp:279
msgctxt "A kind of date representation"
msgid "Compact date"
msgstr "កាល​បរិច្ឆេទ​តិច​"

#: clock.cpp:280
msgctxt "A kind of date representation"
msgid "Short date"
msgstr "កាលបរិច្ឆេទ​ខ្លី​"

#: clock.cpp:281
msgctxt "A kind of date representation"
msgid "Long date"
msgstr "កាល​បរិច្ឆេទ​វែង​"

#: clock.cpp:282
msgctxt "A kind of date representation"
msgid "ISO date"
msgstr "កាលបរិច្ឆេទ ISO​"

#: clock.cpp:476
msgctxt "@label Compact date: %1 day in the month, %2 month number"
msgid "%1/%2"
msgstr "%1/%2"

#: clock.cpp:491
msgctxt ""
"@label Date with currentTimezone: %1 day of the week with date, %2 "
"currentTimezone"
msgid "%1 %2"
msgstr "%1 %2"

#. i18n: file: clockConfig.ui:26
#. i18n: ectx: property (text), widget (QLabel, label_6)
#: rc.cpp:3
msgid "Font"
