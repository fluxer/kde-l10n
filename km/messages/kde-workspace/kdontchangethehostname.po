#: khostname.cpp:71
msgid "Error: HOME environment variable not set.\n"
msgstr "កំហុស ៖ អថេរ​បរិស្ថាន​​ផ្ទះ​មិន​បាន​កំណត់​ទេ ។\n"

#: khostname.cpp:81
msgid "Error: DISPLAY environment variable not set.\n"
msgstr "កំហុស ៖ អថេរ​បរិស្ថាន​បង្ហាញ​មិន​បាន​កំណត់​ទេ ។\n"

#: khostname.cpp:221
msgid "KDontChangeTheHostName"
