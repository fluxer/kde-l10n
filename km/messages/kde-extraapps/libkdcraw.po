#: libkdcraw/dcrawsettingswidget.cpp:591
msgctxt "@item:inlistbox Noise Reduction"
msgid "Wavelets"
msgstr "Wavelets"

#: libkdcraw/dcrawsettingswidget.cpp:592
msgctxt "@item:inlistbox Noise Reduction"
