msgid "Calendar Decorations"
msgstr "Decoracións de calendario"

#: koprefsdialog.cpp:1281
msgctxt "@title:group"
msgid "Other Plugins"
msgstr "Outros engadidos"

#: koprefsdialog.cpp:1350
