#: katesessionapplet.cpp:111
msgid "Start Kate (no arguments)"
msgstr "A Kate elindítása (argumentumok nélkül)"

#: katesessionapplet.cpp:120
msgid "New Kate Session"
msgstr "Új Kate-munkamenet"

#: katesessionapplet.cpp:129
msgid "New Anonymous Session"
msgstr "Új anonymous munkamenet"

#: katesessionapplet.cpp:173
msgid "Session Name"
msgstr "Munkamenet"

#: katesessionapplet.cpp:174
msgid "Please enter a name for the new session"
msgstr "Adjon nevet az új munkamenetnek"

#: katesessionapplet.cpp:181
msgid ""
"An unnamed session will not be saved automatically. Do you want to create "
"such a session?"
msgstr ""
"Létre szeretne hozni egy névtelen munkamenetet? Ezek nem mentődnek el "
"automatikusan."

#: katesessionapplet.cpp:183
msgid "Create anonymous session?"
msgstr "Létrehozás?"

#: katesessionapplet.cpp:190
msgid "You already have a session named %1. Do you want to open that session?"
msgstr "Már létezik %1 nevű munkamenet. Meg szeretné azt nyitni?"

#: katesessionapplet.cpp:191
msgid "Session exists"
msgstr "Már létezik"

#: katesessionapplet.cpp:213
msgid "Sessions"
msgstr "Munkamenetek"

#: katesessionapplet.cpp:215
msgid "Sessions to show"
msgstr "Megjelenítendő munkamenetek"
