msgid "Solid white"
msgstr "Sima fehér"

#: libkdcraw/dcrawsettingswidget.cpp:453
msgctxt "@item:inlistbox"
msgid "Unclip"
msgstr "Levágás vissza"

#: libkdcraw/dcrawsettingswidget.cpp:454
msgctxt "@item:inlistbox"
msgid "Blend"
msgstr "Színkeverés"

