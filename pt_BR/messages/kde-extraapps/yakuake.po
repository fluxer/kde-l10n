#. i18n: file: app/config/windowsettings.ui:169
#. i18n: ectx: property (suffix), widget (QSpinBox, kcfg_Height)
#: rc.cpp:73 rc.cpp:278 rc.cpp:285
#, no-c-format
msgctxt "@label Spinbox unit suffix"
msgid "%"
msgstr "%"

