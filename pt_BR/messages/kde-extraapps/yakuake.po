#: rc.cpp:127
msgctxt "@label"
msgid "Keep window on top"
msgstr "Manter a janela acima das outras"

#. i18n: file: app/config/yakuake.kcfg:51
#. i18n: ectx: whatsthis, entry (KeepAbove), group (Window)
#: rc.cpp:130
msgctxt "@info:whatsthis"
