#: alarmdialog.cpp:165
msgctxt "@title:window"
msgid "Reminders"
msgstr "Lembretes"

#: alarmdialog.cpp:169
msgctxt "@action:button"
msgid "Dismiss Reminder"
msgstr "Descartar lembrete"

#: alarmdialog.cpp:171
msgctxt "@info:tooltip"
msgid "Dismiss the reminders for the selected incidences"
msgstr "Descartar os lembretes para as incidências selecionadas"

#: alarmdialog.cpp:172
msgctxt "@action:button"
msgid "Dismiss All"
msgstr "Descartar todos"

#: alarmdialog.cpp:174
msgctxt "@info:tooltip"
msgid "Dismiss the reminders for all listed incidences"
